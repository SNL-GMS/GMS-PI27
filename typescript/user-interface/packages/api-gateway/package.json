{
  "name": "@gms/api-gateway",
  "version": "0.1.0",
  "description": "GMS API Gateway",
  "keywords": [],
  "license": "UNLICENSED",
  "author": "GMS Development Team",
  "sideEffects": false,
  "main": "dist/api-gateway-server.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean:dist": "yarn rimraf dist/",
    "clean:build": "yarn rimraf lib/ dist/ docs/ coverage/ bundle-analyzer/ artifacts/ .cache/ junit.xml",
    "clean": "npm-run-all -s clean:build",
    "build:eslint": "yarn eslint -c .eslintrc-config.yaml 'src/**/*.{ts,tsx}' --quiet --cache --cache-strategy content --cache-file .cache/.eslintcache",
    "build:eslint:test": "yarn eslint -c .eslintrc-config-test.yaml '{__tests__,__mocks__}/**/*.{ts,tsx}' --quiet --cache --cache-strategy content --cache-file .cache/.eslintcache-test",
    "build": "yarn tsc -b ./tsconfig-build.json",
    "build:test": "yarn tsc -b ./tsconfig-test.json",
    "watch": "yarn tsc -b ./tsconfig-build.json -watch",
    "build:webpack:dev": "webpack --color --env development --config webpack.config.ts --stats-error-details",
    "build:webpack:prod": "webpack --color --env production --config webpack.config.ts --stats-error-details",
    "bundle:dev": "npm-run-all -s clean:dist build:webpack:dev",
    "bundle:prod": "npm-run-all -s clean:dist build:webpack:prod",
    "bundle": "yarn bundle:dev",
    "dev": "yarn bundle:dev && NODE_ENV=development node --max-old-space-size=8142 --inspect-brk --harmony --nolazy ./dist/api-gateway-server.js",
    "start": "yarn bundle:prod && NODE_ENV=production node --max-old-space-size=8142 ./dist/api-gateway-server.js",
    "docs": "yarn clean:docs && typedoc --out docs/ts . && mkdir -p build",
    "sonar": "echo \"run 'yarn sonar' from the top-level directory\"",
    "test": "yarn test:dev",
    "test:dev:jest": "jest --maxWorkers=50% --verbose --bail",
    "test:prod:jest": "jest --maxWorkers=4 --silent --bail --coverageReporters=lcovonly --reporters=default --reporters=jest-junit",
    "test:dev": "npm-run-all -p build:test test:dev:jest",
    "test:prod": "npm-run-all -p build:test test:prod:jest",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "@gms/common-model": "0.1.0",
    "@gms/common-util": "0.1.0",
    "@gms/webpack-config": "0.1.0",
    "axios": "0.22.0",
    "body-parser": "1.20.0",
    "config": "3.3.7",
    "express": "4.18.2",
    "immutable": "4.0.0-rc.12",
    "kafkajs": "2.2.0",
    "kafkajs-snappy": "1.1.0",
    "lodash": "4.17.21",
    "logform": "2.4.2",
    "moment": "2.29.4",
    "msgpack-lite": "0.1.26",
    "node-fetch": "2.6.0",
    "ts-node": "10.9.1",
    "winston": "3.8.1",
    "winston-daily-rotate-file": "4.7.1",
    "ws": "8.14.2",
    "xss-filters": "1.2.7"
  },
  "devDependencies": {
    "@blueprintjs/eslint-plugin": "6.0.3",
    "@types/express": "4.17.2",
    "@types/jest": "29.1.1",
    "@types/lodash": "4.14.149",
    "@types/msgpack-lite": "0.1.7",
    "@types/node": "18.11.9",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "concat": "1.0.3",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-import-resolver-webpack": "0.13.8",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-jsdoc": "46.9.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-workspaces": "0.10.0",
    "jest": "29.1.2",
    "jest-cli": "29.1.2",
    "jest-junit": "12.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.1",
    "prettier-2": "npm:prettier@2.8.8",
    "react": "18.2.0",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.1",
    "ts-loader": "9.2.8",
    "typedoc": "0.15.3",
    "typescript": "4.2.4",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4"
  },
  "jest-junit": {
    "suiteNameTemplate": "api-gateway/{filename}",
    "classNameTemplate": "api-gateway/{filename}/{classname}",
    "titleTemplate": "{title}"
  }
}
